INSERT INTO movie_stars(name) VALUEs ("Angelina Jolie");
CREATE TABLE movies (id INTEGER PRIMARY KEY AUTOINCREMENT, name TEXT);
INSERT INTO movies(name) VALUES ("Pirates of the Caribean");
INSERT INTO movies(name)VALUES ("World War Z");
